{
    "name": "amethyst/setting",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-setting", "setting"],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "amethyst/core": "0.3.*",
        "amethyst/user": "0.3.*"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "amethyst/authentication": "0.3.*",
        "friendsofphp/php-cs-fixer": "^3.52"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "setting",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\SettingServiceProvider"
            ]
        }
    }
}
